{
    "n_gpu": 1,
    "optimizers": {
        "generator": {
            "type": "Adam",
            "args": {
                "lr": 0.0002,
                "betas": [0.5, 0.999]
            }
        },
        "discriminator": {
            "type": "Adam",
            "args": {
                "lr": 0.0002,
                "betas": [0.5, 0.999]
            }
        }
    },
    "lr_schedulers": {
        "generator": {
            "type": "StepLR",
            "args": {
                "step_size": 9999,
                "gamma": 1.0
            }
        },
        "discriminator": {
            "type": "StepLR",
            "args": {
                "step_size": 9999,
                "gamma": 1.0
            }
        }
    },
    "trainer": {
        "epochs": 300,
        "save_dir": "saved/",
        "save_freq": 1,
        "monitor_mode": "min",
        "monitored_loader": "valid_mnist",
        "monitored_metric": "avg_loss"
    },
    "trainer_args": {},
    "visualization": {
        "tensorboardX": true,
        "log_dir": "saved/runs"
    },
    "arch": {
        "type": "MnistGAN",
        "args": {
            "spectral_normalization": false
        }
    },
    "optimize_strategy": "GAN",
    "data_loader": {
        "type": "MnistDataLoader",
        "args": {
            "data_dir": "../datasets/mnist",
            "batch_size": 128,
            "validation_split": 0.0,
            "num_workers": 4,
            "img_size": 64,
            "name": "mnist",
            "norm_std": [0.5],
            "norm_mean": [0.5]
        }
    },
    "losses": {
    },
    "gan_losses": {
        "generator": {
            "type": "GANLoss",
            "args": {
                "nickname": "nsgan_loss_generator",
                "weight": 1,
                "network": "generator",
                "type_": "nsgan"
            }
        },
        "discriminator": {
            "type": "GANLoss",
            "args": {
                "nickname": "nsgan_loss_discriminator",
                "weight": 1,
                "network": "discriminator",
                "type_": "nsgan"
            }
        }
    },
    "metrics": {
    },
    "log_step": 200,
    "verbosity": 2,
    "name": "GAN_template_train_config"
}
